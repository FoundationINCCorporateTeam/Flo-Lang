# Basic arithmetic and variables in Flo

# Variables
x = 10
y = 20
sum = x + y

print("x =", x)
print("y =", y)
print("sum =", sum)

# Functions
func add(a, b) => a + b

result = add(5, 3)
print("add(5, 3) =", result)

# Control flow
if sum > 25 => print("Sum is greater than 25")
else => print("Sum is 25 or less")

# Loops
print("Counting to 5:")
for i range(1, 6) {
    print(i)
}
